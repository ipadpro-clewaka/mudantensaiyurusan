<body>
  <header>
    <div class="banner">
      <button class="btn btn-link" onclick="window.location.href='/home.pdf'">ホーム🏠</button>
      <button class="btn btn-link" onclick="window.location.href='/wakamehistory'">履歴</button>
      <form class="d-flex" action="/s.pdf" method="get">
          <input id="searchbox" name="q" class="form-control" type="text" placeholder="検索..." aria-label="Search" value="<%= query %>" required>
      </form>

     <h3 class="yukiyoutube">wakametube-Custom</h3>

    </div>
  </header>
  <div class="results">
    <% res.items.forEach(video => { %>
      <% if (video.type === 'video') { %>
        <div class="result-item">
          <a href="/w/<%= video.id %>/5.pdf">
            <img loading="lazy" src="/vi/<%= video.id %>/mqdefault.jpg" alt="<%= video.title %>">
          </a>
          <p class="channel-name"><a href="/c.pdf/<%= video.author.channelID %>"><%= video.author.name %></a></p>
          <p class="video-title"><%= video.title %></p>
          <p class="video-description"><%= video.description %></p>
          <p class="video-meta"><%= video.uploadedAt %> - <%= video.duration %></p>
          </div>
      <% } else if (video.type === 'playlist') { %>
        <div class="playresult-item">
          <a href="/p/<%= video.playlistID %>">
            <img style="opacity: 0.5" src="<%= video.firstVideo.bestThumbnail.url.slice(19) %>" alt="<%= video.title %>">
          </a>
          <p class="video-title">プレイリスト(<%= video.length %>本以上の動画)</p>
          <p class="channel-name"><a href="/u/<%= video.owner.id %>"><%= video.owner.name %></a></p>
        </div>
      <% } else if (video.type === 'channel') { %>
        <div class="result-item">
          <a href="/c.pdf/<%= video.channelID %>">
            <img src="https://uploads.scratch.mit.edu/get_image/project/1068152212_144x144.png" alt="<%= video.name %>">
          </a>
          <p class="video-title"><%= video.name %></p>
          <p class="video-description"><%= video.description || video.descriptionShort %></p>
          <p class="video-meta"><%= video.subscribers %> subscribers</p>
        </div>
      <% } %>
    <% }); %>
  </div>

  <div class="pagination">
    <a class="btn" href="/s.pdf?q=<%= query %>&p=<%= page+1 %>">下へ行く</a>
  </div>

      

  <style>
    .pagination {
      display: flex;
      justify-content: space-between;
      padding: 20px;
    }

    a {
      color: #ff8181;
    }

